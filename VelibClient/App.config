<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IVelibService" />
            </basicHttpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IVelibService" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/IWSVelib/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IVelibService"
                contract="VelibService.IVelibService" name="BasicHttpBinding_IVelibService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/IWSVelibEventsLib/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IVelibService"
                contract="VelibServiceEvents.IVelibService" name="WSDualHttpBinding_IVelibService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>